{
  "name": "backend",
  "main": "server.js",
  "scripts": {
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules --copy-files",
    "start": "npm run build && nodemon dist/server.js",
    "serve": "npm run build && node dist/server.js",
    "dev": "nodemon --exec babel-node server.js"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "mongodb": "^3.5.5",
    "mongoose": "^5.9.6",
    "nodemon": "^2.0.2",
    "query-string": "^6.11.1",
    "rimraf": "^3.0.2"
  }
}
